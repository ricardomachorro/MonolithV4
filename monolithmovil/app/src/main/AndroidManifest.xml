<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.alumno.monolithmovil">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/queen"
        android:label="@string/monolith"
        android:roundIcon="@mipmap/queen"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Registrate" />
        <activity android:name=".RecuperarPassword" />
        <activity
            android:name=".MenuPrincipal"
            android:label="@string/title_activity_menu_principal"
            android:theme="@style/AppTheme"/>
    </application>

</manifest>